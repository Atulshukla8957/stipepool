<p id="notice"><%= notice %></p>
<div class="panel panel-default">
<div class="panel-heading">Employee List</div>
<div class="panel-body">
  <table class="table"">
    <thead>
      <tr>
        <th >Username</th>
        <th >Email</th>
        <th colspan="3"></th>
      </tr>
    </thead>

  <tbody>
    <% @branch.employees.each do |branch| %>
    <tr>
      <td ><%= branch.username %></td>
		    <td ><%= branch.email %></td>
    <% end %>
  </tbody>
  </table>
</div>

